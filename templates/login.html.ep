<html>
  <head>
    <meta http-equiv="Content-Type" content="text/html;charset=UTF-8" >
    <title>Login</title>
    <link rel='stylesheet' href='https://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800&#038;subset=latin,latin-ext' type='text/css' media='all'>
  </head>
  <body>
  <div class="main-content" id="main-content">
    <h1>Login Required</h1>
      <div class="main-login-form" id="main-login-form">
        <form method="post" action="<%= url_for('login') %>" enctype ="multipart/form-data">
          <div class="loginemail-field">
           <strong>Username:</strong>  <input type="text" name="user">
          </div>
          <div class="loginemail-field">
            <strong>Password:</strong> <input type="password" name="pass">
          </div>
           <div class="loginemail-submit">
            <input type="submit" value="Login">
          </div>
        </form>
      </div>
    </div>
  </body>
</html>

